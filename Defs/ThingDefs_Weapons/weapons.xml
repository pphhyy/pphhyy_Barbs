  <ThingDef ParentName="BaseMeleeWeapon_BluntBarb_Quality">
    <defName>pphhyy_Barbarian_BoneClub</defName>
    <label>Rusted Bone Club</label>
    <techLevel>Medieval</techLevel>
    <description>A Bone Club made from the bones of animals or slain enemies..</description>
    <graphicData>
      <texPath>Weapons/Barbarian_BoneClub</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>1.0</drawSize>
    </graphicData>
    <uiIconScale>1.2</uiIconScale>
    <weaponTags>
      <li>NeolithicMeleeBasic</li>
    </weaponTags>
    <weaponClasses>
      <li>Neolithic</li>
    </weaponClasses>
    <costList>
      <DankPyon_Bone>20</DankPyon_Bone>
      <!-- <WoodLog>20</WoodLog> -->
    </costList>
    <statBases>
      <WorkToMake>4000</WorkToMake>
      <Mass>1</Mass>
    </statBases>
    <equippedAngleOffset>-65</equippedAngleOffset>
    <burnableByRecipe>True</burnableByRecipe>
    <tools>
      <li>
        <label>handle</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>9</power>
        <cooldownTime>2</cooldownTime>
      </li>
      <li>
        <label>head</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>12</power>
        <cooldownTime>2</cooldownTime>
        <armorPenetration>0.2</armorPenetration>
      </li>
    </tools>
    <recipeMaker>
    <researchPrerequisite>pphhyy_Barbarians_Research</researchPrerequisite>
      <recipeUsers>
        <li>pphhyy_Barbarians_Building_WeaponBench</li>
      </recipeUsers>
    </recipeMaker>
    <comps>
      <li Class="CompOversizedWeapon.CompProperties_OversizedWeapon">
        <verticalFlipNorth>false</verticalFlipNorth>
        <angleAdjustmentNorth>-160</angleAdjustmentNorth>
        <angleAdjustmentEast></angleAdjustmentEast>
        <angleAdjustmentWest></angleAdjustmentWest>
        <angleAdjustmentSouth></angleAdjustmentSouth>

        <northOffset>(0.0,-1,0.1)</northOffset>  
        <southOffset>(-0.0,0,0.0)</southOffset>
        <westOffset>(0.0,0,0.0)</westOffset>
        <eastOffset>(-0.0,0,0.0)</eastOffset>
      </li>
    </comps>
    <modExtensions>
      <li MayRequire="OskarPotocki.VanillaFactionsExpanded.Core" Class="VFECore.ThingDefExtension">
        <usableWithShields>true</usableWithShields>
      </li>
    </modExtensions>
  </ThingDef>

