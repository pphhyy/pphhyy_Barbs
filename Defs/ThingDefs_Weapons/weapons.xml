  <RecipeDef>
    <defName>pphhyy_Barbarian_MakeRustedSword</defName>
    <label>make Rusted Sword</label>
    <description>Make a rusted sword made from a weapon looted on the battlefield.</description>
    <jobString>Making Rusted Sword.</jobString>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <workSkill>Crafting</workSkill>
    <effectWorking>Smith</effectWorking>
    <soundWorking>Recipe_Smith</soundWorking>
    <allowMixingIngredients>true</allowMixingIngredients>
    <workAmount>1000</workAmount>
    <targetCountAdjustment>1</targetCountAdjustment>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>MeleeWeapon_Gladius</li>
            <li>MeleeWeapon_LongSword</li>
            <li>DankPyon_MeleeWeapon_Falchion</li>
            <li>DankPyon_MeleeWeapon_ArmingSword</li>
            <li>DankPyon_MeleeWeapon_FencingSword</li>
            <li>DankPyon_MeleeWeapon_NobleSword</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
	  <li>
        <filter>
          <thingDefs>
            <li>Skull</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>MeleeWeapon_Gladius</li>
        <li>MeleeWeapon_LongSword</li>
        <li>DankPyon_MeleeWeapon_Falchion</li>
        <li>DankPyon_MeleeWeapon_ArmingSword</li>
        <li>DankPyon_MeleeWeapon_FencingSword</li>
        <li>DankPyon_MeleeWeapon_NobleSword</li>
        <li>Skull</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <pphhyy_Barbarian_RustedSword>1</pphhyy_Barbarian_RustedSword>
    </products>
	<!-- <researchPrerequisite>Brachyura_Breeding_I</researchPrerequisite> -->
  </RecipeDef>

